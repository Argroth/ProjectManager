{
  "name": "Telemond",
  "description": "Distributed System - Telemond-Holding",
  "author": {
    "name": "≈Åukasz Gronczakiewicz"
  },
  "scripts": {
    "client": "cd view && yarn start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd view && yarn build && cd .. && yarn start",
    "start": "node server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "mongodb": "^3.2.1",
    "mongoose": "^5.4.19"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
